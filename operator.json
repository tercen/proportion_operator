{
  "name": "Count and Proportion",
  "description": "Calculates counts and relative proportions per row, column, and overall.",
  "tags": [
    "preprocessing",
    "descriptive statistics"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/proportion_operator"
  ],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Row Factor",
            "type": "",
            "description": "(Optional) Variable factor such as sample or group of interest.",
            "ontologyMapping": "variable",
            "crosstabMapping": "row",
            "cardinality": "0..n",
            "factors": []
          },
          {
            "kind": "MetaFactor",
            "name": "Column Factor",
            "type": "",
            "description": "(Optional) Variable factor such as cluster.",
            "ontologyMapping": "variable",
            "crosstabMapping": "column",
            "cardinality": "0..n",
            "factors": []
          }
        ],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "Input Values",
                "type": "numeric",
                "description": "Numeric input values for proportion calculation.",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              }
            ]
          }
        ]
      }
    ]
  }
}